{
  "name": "@omniflex/module-identity-core",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias"
  },
  "exports": {
    "./controllers": {
      "development": "./controllers/index.ts",
      "import": "./dist/controllers/index.js"
    },
    "./validation": {
      "development": "./validation/*.ts",
      "import": "./dist/validation/*.js"
    },
    ".": {
      "development": "./index.ts",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "@omniflex/core": "0.1.0"
  }
}