{
  "name": "@omniflex/core",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias"
  },
  "exports": {
    "./types/*": {
      "development": "./types/*.ts",
      "import": "./dist/types/*.js"
    },
    "./containers": {
      "development": "./containers.ts",
      "import": "./dist/containers.js"
    },
    "./config": {
      "development": "./config.ts",
      "import": "./dist/config.js"
    },
    ".": {
      "development": "./index.ts",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.9.0"
  }
}